@Library('jenkins-pipeline-shared-lib-sample')_

stage('Print Build Info') {
    printBuildinfo {
        name = "Sample Name"
    }
} stage('Snow') {
    disableBalancerUtils()
} stage('Deploy') {
    deploy()
} stage('Terraform') {
    enableBalancerUtils()
} stage('Check Status') {
    checkStatus()
}
